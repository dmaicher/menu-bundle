{
    "name": "dama/menu-bundle",
    "description": "",
    "keywords": [],
    "homepage": "https://github.com/dmaicher/menu-bundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "David Maicher",
            "email": "mail@dmaicher.de"
        }
    ],
    "require": {
        "php": "^7.2 || ^8.0",
        "symfony/framework-bundle": "^4.4 || ^5.3",
        "symfony/twig-bundle": "^4.4 || ^5.3",
        "twig/twig": "^1.35 || ^2.4.4 || ^3.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^0.12",
        "phpunit/phpunit": "^8.5 || ^9.5",
        "symfony/browser-kit": "^4.4|| ^5.3",
        "symfony/css-selector": "^4.4 || ^5.3",
        "symfony/expression-language": "^4.4 || ^5.3",
        "symfony/phpunit-bridge": "^5.3",
        "symfony/security-bundle": "^4.4 || ^5.3",
        "symfony/yaml": "^4.4 || ^5.3"
    },
    "autoload": {
        "psr-4": {
            "DAMA\\MenuBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": { "Tests\\": "tests/" }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.x-dev"
        }
    }
}
