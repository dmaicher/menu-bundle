{
    "name": "dama/menu-bundle",
    "description": "",
    "keywords": [],
    "homepage": "https://github.com/dmaicher/menu-bundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "David Maicher",
            "email": "mail@dmaicher.de"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^7.2",
        "symfony/framework-bundle": "^3.4 || ^4.4 || ^5.0",
        "symfony/twig-bundle": "^3.4 || ^4.4 || ^5.0",
        "symfony/yaml": "^3.4 || ^4.4 || ^5.0",
        "twig/twig": "^1.0 || ^2.0 || ^3.0"
    },
    "require-dev": {
        "phpstan/phpstan-shim": "^0.11",
        "phpunit/phpunit": "^8.4",
        "symfony/browser-kit": "^3.4 || ^4.4|| ^5.0",
        "symfony/css-selector": "^3.4 || ^4.4 || ^5.0",
        "symfony/expression-language": "^3.4 || ^4.4 || ^5.0",
        "symfony/phpunit-bridge": "^5.1",
        "symfony/security-bundle": "^3.4 || ^4.4 || ^5.0"
    },
    "autoload": {
        "psr-4": {
            "DAMA\\MenuBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": { "Tests\\": "tests/" }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.2.x-dev"
        }
    }
}
